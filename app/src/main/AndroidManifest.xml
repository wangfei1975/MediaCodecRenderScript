<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wterry.fei.renderscriptdemo" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DemoNV12ToARGBActivity"
            android:label="@string/title_activity_demo_nv12_to_argb" >
        </activity>
        <activity
            android:name=".DemoARGBToNV12Activity"
            android:label="@string/title_activity_demo_argbto_nv12" >
        </activity>
        <activity
            android:name=".DemoNV12ImageProcessorActivity"
            android:label="@string/title_activity_demo_nv12_image_processor" >
        </activity>
        <activity
            android:name=".DemoNV12BlitBitmapActivity"
            android:label="@string/title_activity_demo_nv12_blit_bitmap" >
        </activity>
        <activity
            android:name=".FilterSelectorActivity"
            android:label="@string/title_activity_filter_selector" >
        </activity>
    </application>

</manifest>
